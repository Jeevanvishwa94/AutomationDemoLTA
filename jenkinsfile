pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/Jeevanvishwa94/AutomationDemoLTA.git'
            }
        }
        stage('Test') {
            steps {
                'pytest -v testcase\test_play_childrens_lesson.py'
            }
        }
        stage('Deploy') {
            steps {
                'pytest -v testcase\test_lta_login.py'
            }
        }
    }
}
